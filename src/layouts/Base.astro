<script is:inline src="/js/bootstrap-popover.js"/>
<script is:inline src="/js/bootstrap-button.js"/>
<script is:inline src="/js/bootstrap-collapse.js"/>
<script is:inline src="/js/bootstrap-carousel.js"/>
<script is:inline src="/js/bootstrap-typeahead.js"/>
